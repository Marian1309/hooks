{
  "name": "@pidchashyi/hooks",
  "version": "3.0.3",
  "description": "A collection of hooks for React",
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "homepage": "https://github.com/Marian1309/hooks#readme",
  "bugs": {
    "url": "https://github.com/Marian1309/hooks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marian1309/hooks.git"
  },
  "license": "MIT",
  "author": "Marian Pidchashyi",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dom": {
      "import": "./dist/dom.js",
      "require": "./dist/dom.js",
      "types": "./dist/dom.d.ts"
    },
    "./advanced": {
      "import": "./dist/advanced.js",
      "require": "./dist/advanced.js",
      "types": "./dist/advanced.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "postpublish": "rimraf -rf dist"
  },
  "dependencies": {
    "@pidchashyi/try-catch": "^1.1.0",
    "nuqs": "^2.4.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "typescript": "^5.0.0"
  }
